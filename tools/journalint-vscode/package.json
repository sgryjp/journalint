{
  "name": "journalint",
  "displayName": "journalint",
  "publisher": "sgryjp",
  "author": {
    "name": "Suguru Yamamoto",
    "email": "sgryjp@gmail.com",
    "url": "https://github.com/sgryjp"
  },
  "description": "Linter for my personal journal files.",
  "license": "(MIT OR Apache-2.1)",
  "version": "0.2.0",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/sgryjp/journalint.git"
  },
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "node scripts/compile.js",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.76.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "shelljs": "^0.8.5",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "vscode-languageclient": "^8.1.0"
  }
}
